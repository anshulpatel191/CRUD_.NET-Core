import React, { useContext, useState } from "react";
import axios from "axios";
import { AppContext } from '../Context/App.context';
import { useNavigate } from "react-router-dom";
import { toast, ToastContainer } from "react-toastify";
import "react-toastify/dist/ReactToastify.css"; // Import the CSS for styling

const RegisterPage = () => {
  const [name, setName] = React.useState('');
  const [dob, setDob] = React.useState('');
  const [phone, setPhone] = React.useState('');
  const [email, setEmail] = React.useState('');
  const [custpassword, setPwd] = React.useState('');
  const [address, setAddress] = React.useState('');
  const [city, setCity] = React.useState('');
  const [aadhaar, setAadhaar] = React.useState('');
  const [passwordType, setPasswordType] = React.useState('password');
  const [Error, setError] = React.useState('');
  const { user, setUser } = useContext(AppContext);
  const navigate = useNavigate();

  const handleName = (event) => {
    setName(event.target.value);
  }

  const handleDob = (event) => {
    setDob(event.target.value);
  }

  const handlePhone = (event) => {
    setPhone(event.target.value);
  }

  const handleAddress = (event) => {
    setAddress(event.target.value);
  }

  const handleCity = (event) => {
    setCity(event.target.value);
  }

  const handleAadhaar = (event) => {
    setAadhaar(event.target.value);
  }

  const handleEmail = (event) => {
    setEmail(event.target.value);
  }

  const handlePwd = (event) => {
    setPwd(event.target.value);
  }

  const togglePasswordVisibility = () => {
    setPasswordType(passwordType === 'password' ? 'text' : 'password');
  }

  const handleRegister = async (event) => {
    event.preventDefault();
    const res1 = {
      "customerName": name,
      "dob": dob,
      "phoneNumber": phone,
      "email": email,
      "customerAddress": address,
      "city": city,
      "aadhaarNumber": aadhaar,
    };
    const res2 = {
      "userEmail": email,
      "userPassword": custpassword,
      "userRole": "CUSTOMER"
    };
    const res = { "user": res1, "auth": res2 };

    try {
      const response = await axios.post('CustomerRegistration', res);
      // Assuming the response contains a success message
      const successMessage = response.data.message;

      // Show a success toast notification
      toast.success(successMessage, {
        position: toast.POSITION.TOP_CENTER,
        autoClose: 3000, // Auto close the toast after 3 seconds
      });
      
      // You can also navigate to a different page or perform other actions here

    } catch (error) {
      setError(error.message);
      // Show an error toast notification
      toast.error("Registration failed. Please try again.", {
        position: toast.POSITION.TOP_CENTER,
        autoClose: 3000, // Auto close the toast after 3 seconds
      });
    }
  }

  return (
    <div className="container">
      <form>
        <h1 className="mt-4">New Customer Registration Page</h1>
        {/* ... (your form fields) */}
        <div className="form-group text-center">
          <button type="submit" className="btn btn-primary" onClick={handleRegister}>Register</button>
        </div>
      </form>
      {/* Add the ToastContainer component to display toasts */}
      <ToastContainer />
    </div>
  )
}

export default RegisterPage;

import React, {useContext} from "react";
import { AppContext } from "../Context/App.context";
import { useNavigate } from "react-router-dom";
import {useParams} from 'react-router-dom';

const Success = () => {    
    const { user,handleUserInfo } = useContext(AppContext);
    /*const navigate = useNavigate();
    const handleSubmit = () =>{
        navigate('/');
    }*/
    const {message, name} = useParams();
    
    return (
        <div>
            <h1> Success! </h1>
            <p>Message : {message}</p>
            <p>Nmae : {name}</p>

            {/*button onClick={handleSubmit}> Logout </button>*/}
            <button onClick={() => { handleUserInfo(null) }}> Logout </button>
        </div>
    )
}
export default Success;


import React, {useContext,useState} from "react";
import axios from "axios";
import { AppContext } from '../Context/App.context';
import { useNavigate } from "react-router-dom";
import 'bootstrap/dist/css/bootstrap.min.css';
import 'bootstrap/dist/js/bootstrap.min.js';
import { toast, ToastContainer } from "react-toastify";
import "react-toastify/dist/ReactToastify.css";
//import {useHistory} from 'react-router-dom';
import { Accordion } from "react-bootstrap";

const RegisterPage = () => {
  const [name, setName] = React.useState('');
  const [dob, setDob] = React.useState('');
  const [phone, setPhone] = React.useState('');
  const [email, setEmail] = React.useState('');
  const [custpassword, setPwd] = React.useState('');
  const [address, setAddress] = React.useState('');
  const [city, setCity] = React.useState('');
  const [aadhaar, setAadhaar] = React.useState('');
  const [passwordType, setPasswordType] = React.useState('password'); // To toggle password visibility
  const [Error, setError] = React.useState('');
  const { user, setUser } = useContext(AppContext);

  const navigate = useNavigate();

  const handleName = (event) => {
    setName(event.target.value);
  }

  const handleDob = (event) => {
    setDob(event.target.value);
  }

  const handlePhone = (event) => {
    setPhone(event.target.value);
  }

  const handleAddress = (event) => {
    setAddress(event.target.value);
  }

  const handleCity = (event) => {
    setCity(event.target.value);
  }

  const handleAadhaar = (event) => {
    setAadhaar(event.target.value);
  }

  const handleEmail = (event) => {
    setEmail(event.target.value);
  }

  const handlePwd = (event) => {
    setPwd(event.target.value);
  }

  const togglePasswordVisibility = () => {
    setPasswordType(passwordType === 'password' ? 'text' : 'password');
  }

 // const history = useHistory();

  const handleRegister = async (event) => {
    event.preventDefault();
    const res1 = {
      "customerName": name,
      "dob": dob,
      "phoneNumber": phone,
      "email": email,
      "customerAddress": address,
      "city": city,
      "aadhaarNumber": aadhaar
    };
    const res2 = {
      "userEmail": email,
      "userPassword": custpassword,
      "userRole": "CUSTOMER"
    };
    const res = { "user": res1, "auth": res2 };

    try {
      axios.post('CustomerRegistration', res)
        .then((response) => {
          console.log(response.data);

          const accnt_num_new = response.data;

          // Show a success toast notification
          toast.success(`Succesfully registered for ${name} with account number : ${accnt_num_new}`, {
            position: toast.POSITION.TOP_CENTER,
            autoClose: 3000, // Auto close the toast after 3 seconds
          });

          navigate(`/success/${accnt_num_new}/${name}`);

        });
    } catch (error) {
      setError(error.message);
    }
  }

  return (
    <div className="container">
      <form>
        <h1 className="mt-4">New Customer Registration Page</h1>
        <div className="form-group  mb-3">
          <label htmlFor="name " className="form-label">Name:</label>
          <input type="text" className="form-control" id="name" value={name} onChange={handleName} placeholder="Enter your Name"/>
        </div>
        <div className="form-group  mb-3">
          <label htmlFor="dob" className="form-label">Date Of Birth:</label>
          <input type="date" className="form-control" id="dob" value={dob} onChange={handleDob} />
        </div>
        <div className="form-group mb-3">
          <label htmlFor="email" className="form-label">Email:</label>
          <input type="email" className="form-control" id="email" value={email} onChange={handleEmail} placeholder="Enter your Email" />
        </div>
        <div className="form-group mb-3">
          <label htmlFor="password" className="form-label">Password:</label>
          <div className="input-group">
            <input type={passwordType} className="form-control" id="password" value={custpassword} onChange={handlePwd} placeholder="Enter your Password" />
            <div className="input-group-append">
              <span className="input-group-text">
                <i
                  className={`bi bi-eye${passwordType === 'password' ? '-slash' : ''}`}
                  onClick={togglePasswordVisibility}
                  style={{ cursor: 'pointer' }}
                ></i>
              </span>
            </div>
          </div>
        </div>
        <div className="form-group mb-3">
          <label htmlFor="phone" className="form-label">Phone Number:</label>
          <input type="text" className="form-control" id="phone" value={phone} onChange={handlePhone} placeholder="Enter your mobile number"/>
        </div>
        <div className="form-group mb-3">
          <label htmlFor="address" className="form-label">Address:</label>
          <input type="text" className="form-control" id="address" value={address} onChange={handleAddress} placeholder="Enter your address" />
        </div>
        <div className="form-group mb-3">
          <label htmlFor="city" className="form-label">City:</label>
          <input type="text" className="form-control" id="city" value={city} onChange={handleCity} placeholder="Enter your city"/>
        </div>
        <div className="form-group mb-3">
          <label htmlFor="aadhaar" className="form-label">Aadhaar Number:</label>
          <input type="text" className="form-control" id="aadhaar" value={aadhaar} onChange={handleAadhaar} placeholder="Enter your 12 digit Adhar Number"/>
        </div>
        <div className="form-group text-center">
          <button type="submit" className="btn btn-primary" onClick={handleRegister}>Register</button>
        </div>
      </form>
      <ToastContainer/>
    </div>
  )
}

export default RegisterPage;

import React from 'react';
import { BrowserRouter, Route, Routes, useNavigate } from 'react-router-dom';
import { AppProvider } from './Context/App.context';
import NavbarComponent from './Components/NavBar';
import LoginWithToken from './Pages/LoginWithToken';
import ProfilePage from './Pages/ProfilePage';
import RegisterPage from './Pages/RegisterPage';
import CustomerPage from './Pages/CustomerPage';
import TransferPage from './Pages/TransferPage';
import Success from './Pages/Success';
import StatementPage from './Pages/StatementPage';
import ProtectedRoute from './Components/ProtectedRoute';
import CustomerDashboard from './Pages/CustomerDashboard';
import AdminDashboard from './Pages/AdminDashboard';
import AccountDashboard from './Pages/AccountDashboard';

const App = () => {
  return (
    <BrowserRouter>
      <AppProvider>
        <NavbarWithNavigate />
        <Routes>
          <Route path="/" element={<LoginWithToken />} />
          <Route path="/login" element={<LoginWithToken />} />
          <Route path="/register" element={<RegisterPage />} />
          <Route
            path="/profile"
            element={
              <ProtectedRoute>
                <ProfilePage />
              </ProtectedRoute>
            }
          />
          <Route
            path="/customer"
            element={
              <ProtectedRoute>
                <CustomerPage />
              </ProtectedRoute>
            }
          />
          <Route
            path="/transfer"
            element={
              <ProtectedRoute>
                <TransferPage />
              </ProtectedRoute>
            }
          />
          <Route
            path="/success/:message/:name"
            element={
              <ProtectedRoute>
                <Success />
              </ProtectedRoute>
            }
          />
          <Route
            path="/account-dashboard"
            element={
              <ProtectedRoute>
                <AccountDashboard />
              </ProtectedRoute>
            }
          />
          <Route path="/statement" element={<StatementPage />} />
          <Route path="/customer-dashboard" element={<CustomerDashboard />} />
          <Route path="/admin-dashboard" element={<AdminDashboard />} />
        </Routes>
      </AppProvider>
    </BrowserRouter>
  );
};

export default App;

const NavbarWithNavigate = () => {
  const navigate = useNavigate();

  return <NavbarComponent navigate={navigate} />;
};

