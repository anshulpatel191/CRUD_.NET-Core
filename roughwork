import React, {useContext,useState} from "react";
import axios from "axios";
import { AppContext } from '../Context/App.context';
import { useNavigate } from "react-router-dom";

const RegisterPage = () => {
    //const [loginobj, setLogin] = useState({UserEmail:'', UserPassword:''});
    const [name, setName] = React.useState('');
    const [dob, setDob] = React.useState('');
    const [phone, setPhone] = React.useState('');
    const [email, setEmail] = React.useState('');
    const [custpassword, setpwd] = React.useState('');
    const [address, setAddress] = React.useState('');
    const [city, setCity] = React.useState('');
    const [aadhaar, setAadhaar] = React.useState('');
    const [Error, setError] = React.useState('');   
    const{user, setUser} = useContext(AppContext);

    const navigate=useNavigate();

    const handleName = (event) => {
        setName(event.target.value);
    }
    const handleDob = (event) => {
        setDob(event.target.value);
    }
    const handlePhone = (event) => {
        setPhone(event.target.value);
    }
    const handleAddress = (event) => {
        setAddress(event.target.value);
    }
    const handleCity = (event) => {
        setCity(event.target.value);
    }
    const handleAadhaar = (event) => {
        setAadhaar(event.target.value);
    }
    const handleEmail = (event) => {
        setEmail(event.target.value);
    }
    const handlepwd = (event) => {
        setpwd(event.target.value);
    }

    const handleRegister = async(event) => {
        //loginobj.UserEmail = email;
        //loginobj.UserPassword = custpassword;
        
        const res1={           
            "customerName": name,
            "dob": dob,
            "phoneNumber": phone,
            "email": email,
            "customerAddress": address,
            "city": city,
            "aadhaarNumber": aadhaar,
        };
        const res2={
            "useremail": email,
            "userPassword": custpassword,
            "userRole": "CUSTOMER"
        };
        const res = {"user":res1, "auth":res2};

        event.preventDefault();
        try{
            axios.post('CustomerRegistration', res)
            .then((response)=>{
                console.log(response.data);
            }
        );}
        catch(error){
            setError(error.Message);
        }
    }   

    return (
        <div align='center'>
            <form>
                <h1> New Customer Registration Page </h1>
                <div>
                    Name: <input type="text" value={name} onChange={handleName} />
                </div>
                <div>
                    Date Of Birth: <input type="date" value={dob} onChange={handleDob} />
                </div>
                <div>
                    Email: <input type="email" value={email} onChange={handleEmail} />
                </div>                
                <div>
                    Password: <input type="password" value={custpassword} onChange={handlepwd} />
                </div>
                <div>
                    Phone Number: <input type="text" value={phone} onChange={handlePhone} />
                </div>
                <div>
                    Address: <input type="text" value={address} onChange={handleAddress} />
                </div>
                <div>
                    City: <input type="text" value={city} onChange={handleCity} />
                </div>
                <div>
                    Aadhaar Number: <input type="text" value={aadhaar} onChange={handleAadhaar} />
                </div>                
                <div>
                    <button type="submit" onClick={handleRegister}> Register </button>
                </div>
            </form>
        </div>
    )
}

export default RegisterPage;





import React, { useContext, useState } from "react";
import axios from "axios";
import { AppContext } from '../Context/App.context';
import { useNavigate } from "react-router-dom";

const RegisterPage = () => {
  const [name, setName] = React.useState('');
  const [dob, setDob] = React.useState('');
  const [phone, setPhone] = React.useState('');
  const [email, setEmail] = React.useState('');
  const [custpassword, setPwd] = React.useState('');
  const [address, setAddress] = React.useState('');
  const [city, setCity] = React.useState('');
  const [aadhaar, setAadhaar] = React.useState('');
  const [passwordType, setPasswordType] = React.useState('password'); // To toggle password visibility
  const [Error, setError] = React.useState('');
  const { user, setUser } = useContext(AppContext);

  const navigate = useNavigate();

  const handleName = (event) => {
    setName(event.target.value);
  }

  const handleDob = (event) => {
    setDob(event.target.value);
  }

  const handlePhone = (event) => {
    setPhone(event.target.value);
  }

  const handleAddress = (event) => {
    setAddress(event.target.value);
  }

  const handleCity = (event) => {
    setCity(event.target.value);
  }

  const handleAadhaar = (event) => {
    setAadhaar(event.target.value);
  }

  const handleEmail = (event) => {
    setEmail(event.target.value);
  }

  const handlePwd = (event) => {
    setPwd(event.target.value);
  }

  const togglePasswordVisibility = () => {
    setPasswordType(passwordType === 'password' ? 'text' : 'password');
  }

  const handleRegister = async (event) => {
    event.preventDefault();
    const res1 = {
      "customerName": name,
      "dob": dob,
      "phoneNumber": phone,
      "email": email,
      "customerAddress": address,
      "city": city,
      "aadhaarNumber": aadhaar,
    };
    const res2 = {
      "useremail": email,
      "userPassword": custpassword,
      "userRole": "CUSTOMER"
    };
    const res = { "user": res1, "auth": res2 };

    try {
      axios.post('CustomerRegistration', res)
        .then((response) => {
          console.log(response.data);
        });
    } catch (error) {
      setError(error.Message);
    }
  }

  return (
    <div className="container">
      <form>
        <h1 className="mt-4">New Customer Registration Page</h1>
        <div className="form-group">
          <label htmlFor="name">Name:</label>
          <input type="text" className="form-control" id="name" value={name} onChange={handleName} />
        </div>
        <div className="form-group">
          <label htmlFor="dob">Date Of Birth:</label>
          <input type="date" className="form-control" id="dob" value={dob} onChange={handleDob} />
        </div>
        <div className="form-group">
          <label htmlFor="email">Email:</label>
          <input type="email" className="form-control" id="email" value={email} onChange={handleEmail} />
        </div>
        <div className="form-group">
          <label htmlFor="password">Password:</label>
          <div className="input-group">
            <input type={passwordType} className="form-control" id="password" value={custpassword} onChange={handlePwd} />
            <div className="input-group-append">
              <span className="input-group-text">
                <i
                  className={`bi bi-eye${passwordType === 'password' ? '-slash' : ''}`}
                  onClick={togglePasswordVisibility}
                  style={{ cursor: 'pointer' }}
                ></i>
              </span>
            </div>
          </div>
        </div>
        <div className="form-group">
          <label htmlFor="phone">Phone Number:</label>
          <input type="text" className="form-control" id="phone" value={phone} onChange={handlePhone} />
        </div>
        <div className="form-group">
          <label htmlFor="address">Address:</label>
          <input type="text" className="form-control" id="address" value={address} onChange={handleAddress} />
        </div>
        <div className="form-group">
          <label htmlFor="city">City:</label>
          <input type="text" className="form-control" id="city" value={city} onChange={handleCity} />
        </div>
        <div className="form-group">
          <label htmlFor="aadhaar">Aadhaar Number:</label>
          <input type="text" className="form-control" id="aadhaar" value={aadhaar} onChange={handleAadhaar} />
        </div>
        <div>
          <button type="submit" className="btn btn-primary" onClick={handleRegister}>Register</button>
        </div>
      </form>
    </div>
  )
}

export default RegisterPage;


import React, { useContext } from "react";
import { AppContext } from '../Context/App.context';
import { useNavigate } from "react-router-dom";

const ProfilePage = () => {
  const { user, handleUserInfo } = useContext(AppContext);
  const navigate = useNavigate();

  const handleRegister = () => {
    navigate('/register');
  }

  return (
    <div className="container mt-5">
      <div className="row">
        <div className="col-md-6 mx-auto">
          <div className="card">
            <div className="card-body">
              <h1 className="card-title text-center mb-4">Admin Profile Page</h1>
              <p className="card-text text-center">Hello {user.userEmail}</p>
              <div className="d-flex justify-content-center">
                <button type="button" className="btn btn-primary mr-2" onClick={handleRegister}>Register</button>
                <button type="button" className="btn btn-danger" onClick={() => { handleUserInfo(null) }}>Logout</button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  )
}

export default ProfilePage;

